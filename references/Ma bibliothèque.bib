  url = {http://scripts.iucr.org/cgi-bin/paper?S0907444910007493},
  urldate = {2022-05-24},
  abstract = {Coot               is a molecular-graphics application for model building and validation of biological macromolecules. The program displays electron-density maps and atomic models and allows model manipulations such as idealization, real-space refinement, manual rotation/translation, rigid-body fitting, ligand search, solvation, mutations, rotamers and Ramachandran idealization. Furthermore, tools are provided for model validation as well as interfaces to external programs for refinement, validation and graphics. The software is designed to be easy to learn for novice users, which is achieved by ensuring that tools for common tasks are `discoverable' through familiar user-interface elements (menus and toolbars) or by intuitive behaviour (mouse controls). Recent developments have focused on providing tools for expert users, with~customisable key bindings, extensions and an extensive scripting interface. The software is under rapid development, but has already achieved very widespread use within the crystallographic community. The current state of the software is presented, with a description of the facilities available and of some of the underlying methods employed.},
  langid = {english},
  file = {C\:\\Users\\nessl\\Zotero\\storage\\K5KWAJGP\\Emsley et al. - 2010 - Features and development of Coot.pdf}
}

@article{espinolaCisregulatoryChromatinLoops2021,
  title = {Cis-Regulatory Chromatin Loops Arise before {{TADs}} and Gene Activation, and Are Independent of Cell Fate during Early {{Drosophila}} Development},
  author = {Espinola, Sergio Martin and Götz, Markus and Bellec, Maelle and Messina, Olivier and Fiche, Jean-Bernard and Houbron, Christophe and Dejean, Matthieu and Reim, Ingolf and Cardozo Gizzi, Andrés M. and Lagha, Mounia and Nollmann, Marcelo},
  date = {2021-04},
  journaltitle = {Nature Genetics},
  shortjournal = {Nat Genet},
  volume = {53},
  number = {4},
  pages = {477--486},
  publisher = {{Nature Publishing Group}},
  issn = {1546-1718},
  doi = {10.1038/s41588-021-00816-z},
  url = {https://www.nature.com/articles/s41588-021-00816-z},
  urldate = {2022-05-14},
  abstract = {Acquisition of cell fate is thought to rely on the specific interaction of remote cis-regulatory modules (CRMs), for example, enhancers and target promoters. However, the precise interplay between chromatin structure and gene expression is still unclear, particularly within multicellular developing organisms. In the present study, we employ Hi-M, a single-cell spatial genomics approach, to detect CRM–promoter looping interactions within topologically associating domains (TADs) during early Drosophila development. By comparing cis-regulatory loops in alternate cell types, we show that physical proximity does not necessarily instruct transcriptional states. Moreover, multi-way analyses reveal that multiple CRMs spatially coalesce to form hubs. Loops and CRM hubs are established early during development, before the emergence of TADs. Moreover, CRM hubs are formed, in part, via the action of the pioneer transcription factor Zelda and precede transcriptional activation. Our approach provides insight into the role of CRM–promoter interactions in defining transcriptional states, as well as distinct cell types.},
  issue = {4},
  langid = {english},
  keywords = {Embryogenesis,Epigenomics,Gene regulation,Microscopy},
  file = {C\:\\Users\\nessl\\Zotero\\storage\\GUNRBLS8\\Espinola et al. - 2021 - Cis-regulatory chromatin loops arise before TADs a.pdf;C\:\\Users\\nessl\\Zotero\\storage\\T28WFTAM\\Espinola et al. - 2021 - Cis-regulatory chromatin loops arise before TADs a.pdf;C\:\\Users\\nessl\\Zotero\\storage\\MWE7HY99\\s41588-021-00816-z.html}
}

