@article{parkFirstSuccessfulCulture2006,
  title = {First Successful Culture of the Marine Dinoflagellate {{Dinophysis}} Acuminata},
  author = {Park, Myung Gil and Kim, Sunju and Kim, Hyung Seop and Myung, Geumog and Kang, Yi Gu and Yih, Wonho},
  date = {2006-11-24},
  journaltitle = {Aquatic Microbial Ecology},
