@article{murshudovREFMAC5RefinementMacromolecular2011,
  title = {{{REFMAC5}} for the Refinement of Macromolecular Crystal Structures},
  author = {Murshudov, G. N. and Skubák, P. and Lebedev, A. A. and Pannu, N. S. and Steiner, R. A. and Nicholls, R. A. and Winn, M. D. and Long, F. and Vagin, A. A.},
  date = {2011-04-01},
  journaltitle = {Acta Crystallographica Section D: Biological Crystallography},
  shortjournal = {Acta Cryst D},
  volume = {67},
  number = {4},
  pages = {355--367},
  publisher = {{International Union of Crystallography}},
  issn = {0907-4449},
  doi = {10.1107/S0907444911001314},
  url = {//scripts.iucr.org/cgi-bin/paper?ba5152},
  urldate = {2022-05-08},
  abstract = {This paper describes various components of the macromolecular crystallographic refinement program REFMAC5, which is distributed as part of the CCP4 suite. REFMAC5 utilizes different likelihood functions depending on the diffraction data employed (amplitudes or intensities), the presence of twinning and the availability of SAD/SIRAS experimental diffraction data. To ensure chemical and structural integrity of~the refined model, REFMAC5 offers several classes of restraints and choices of model parameterization. Reliable models at resolutions at least as low as 4\hspace{0.25em}Å can be achieved thanks to low-resolution refinement tools such as secondary-structure restraints, restraints to known homologous structures, automatic global and local NCS restraints, `jelly-body' restraints and the use of novel long-range restraints on atomic displacement parameters (ADPs) based on the Kullback–Leibler divergence. REFMAC5 additionally offers TLS parameterization and, when high-resolution data are available, fast refinement of anisotropic ADPs. Refinement in the~presence of twinning is performed in a fully automated fashion. REFMAC5 is a flexible and highly optimized refinement package that is ideally suited for refinement across the entire resolution spectrum encountered in macromolecular crystallography.},
  issue = {4},
  langid = {english},
  file = {C\:\\Users\\nessl\\Zotero\\storage\\44G8777B\\Murshudov et al. - 2011 - REFMAC5 for the refinement of macromolecular cryst.pdf;C\:\\Users\\nessl\\Zotero\\storage\\FJF2YL8E\\index.html}
}

